                          <cursor>
                              <Cursor fx:constant="OPEN_HAND" />
                          </cursor>
                          <font>
                              <Font name="System Bold" size="14.0" />
                          </font>
                          <items>
                              <MenuItem onAction="#handleDocumenti" text="Documenti">
                                  <graphic>
                                      <ImageView fitHeight="12.0" fitWidth="12.0" preserveRatio="true">
